@model GreenMap.Models.SkladChemicznyWodPodziemnych

<div class="modal fade bd-example-modal-lg" id="infoChemistryModal" tabindex="-1" role="dialog" aria-labelledby="infoChemistryModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="infoChemistryLabel">Skład chemiczny wód podziemnych</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div>
                    <dl class="row">
                        <dt class="col-sm-6">
                            @Html.DisplayNameFor(model => model.SymbolPunktu)
                        </dt>
                        <dd class="col-sm-6" id="infoChemistrySymbol">
                            @Html.DisplayFor(model => model.SymbolPunktu)
                        </dd>
                        <dt class="col-sm-6">
                            @Html.DisplayNameFor(model => model.X)
                        </dt>
                        <dd class="col-sm-6" id="infoChemistryCoord">
                            @Html.DisplayFor(model => model.X) @Html.DisplayFor(model => model.Y) @Html.DisplayFor(model => model.RzednaTerenu)
                        </dd>
                        <dt class="col-sm-6">
                            @Html.DisplayNameFor(model => model.DataBadania)
                        </dt>
                        <dd class="col-sm-6" id="infoChemistryDate">
                            @Html.DisplayFor(model => model.DataBadania)
                        </dd>
                        <dt class="col-sm-6">
                            @Html.DisplayNameFor(model => model.Ph)
                        </dt>
                        <dd class="col-sm-6" id="infoChemistryPh">
                            @Html.DisplayFor(model => model.Ph)
                        </dd>
                        <dt class="col-sm-6">
                            @Html.DisplayNameFor(model => model.Pew)
                        </dt>
                        <dd class="col-sm-6" id="infoChemistryPew">
                            @Html.DisplayFor(model => model.Pew)
                        </dd>
                        <dt class="col-sm-6">
                            @Html.DisplayNameFor(model => model.Sar)
                        </dt>
                        <dd class="col-sm-6" id="infoChemistrySar">
                            @Html.DisplayFor(model => model.Sar)
                        </dd>
                        <dt class="col-sm-6">
                            @Html.DisplayNameFor(model => model.KlasaJakosci)
                        </dt>
                        <dd class="col-sm-6" id="infoChemistryQuality">
                            @Html.DisplayFor(model => model.KlasaJakosci)
                        </dd>
                        <dt class="col-sm-6">
                            @Html.DisplayNameFor(model => model.PrzydatnoscDoNawadniania)
                        </dt>
                        <dd class="col-sm-6" id="infoChemistryIrrigation">
                            @Html.DisplayFor(model => model.PrzydatnoscDoNawadniania)
                        </dd>
                    </dl>
                </div>
            </div>
        </div>
    </div>
</div>