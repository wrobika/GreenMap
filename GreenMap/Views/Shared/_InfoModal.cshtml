@model GreenMap.Models.OdwiertInfo


<div class="modal fade bd-example-modal-lg" id="infoModal" tabindex="-1" role="dialog" aria-labelledby="infoModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="infoModalLabel">Odwiert</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div>
                    <dl class="row">
                        <dt class="col-sm-4">
                            @Html.DisplayNameFor(model => model.NazwaObiektu)
                        </dt>
                        <dd class="col-sm-8" id="infoName">
                            @Html.DisplayFor(model => model.NazwaObiektu)
                        </dd>
                        <dt class="col-sm-4">
                            @Html.DisplayNameFor(model => model.NrRbdh)
                        </dt>
                        <dd class="col-sm-8" id="infoRbdh">
                            @Html.DisplayFor(model => model.NrRbdh)
                        </dd>
                        <dt class="col-sm-4">
                            @Html.DisplayNameFor(model => model.Lokalizacja)
                        </dt>
                        <dd class="col-sm-8" id="infoDistrict">
                            @Html.DisplayFor(model => model.Lokalizacja)
                        </dd>
                        <dt class="col-sm-4">
                            @Html.DisplayNameFor(model => model.Status)
                        </dt>
                        <dd class="col-sm-8" id="infoStatus">
                            @Html.DisplayFor(model => model.Status)
                        </dd>
                        <dt class="col-sm-4">
                            @Html.DisplayNameFor(model => model.EurefX)
                        </dt>
                        <dd class="col-sm-8" id="infoX">
                            @Html.DisplayFor(model => model.EurefX)
                        </dd>
                        <dt class="col-sm-4">
                            @Html.DisplayNameFor(model => model.EurefY)
                        </dt>
                        <dd class="col-sm-8" id="infoY">
                            @Html.DisplayFor(model => model.EurefY)
                        </dd>
                    </dl>
                </div>
            </div>
        </div>
    </div>
</div>